{
    "name": "@tempos-ai/frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev:setup": "node dev-script.js start",
        "dev": "npm run dev:setup && next dev",
        "dev:restore": "node dev-script.js restore",
        "prebuild": "npm run dev:restore",
        "build": "cross-env NEXT_PUBLIC_EXPORT=true next build && node enhance-static-export.js",
        "build:debug": "cross-env NEXT_PUBLIC_EXPORT=true NODE_OPTIONS='--inspect' next build && node enhance-static-export.js",
        "start": "next start",
        "lint": "next lint",
        "verify-export": "node verify-static-export.js"
    },
    "dependencies": {
        "@heroicons/react": "^2.2.0",
        "@supabase/auth-helpers-nextjs": "^0.7.0",
        "@supabase/auth-helpers-react": "^0.4.0",
        "@supabase/supabase-js": "^2.31.0",
        "@tailwindcss/line-clamp": "^0.4.4",
        "@tailwindcss/typography": "^0.5.16",
        "@tempos-ai/shared": "file:../shared",
        "axios": "^1.8.3",
        "chart.js": "^4.3.0",
        "file-saver": "^2.0.5",
        "formidable": "^3.5.2",
        "glob": "^8.1.0",
        "html2canvas": "^1.4.1",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.5.25",
        "micro": "^10.0.1",
        "next": "^14.0.0",
        "next-auth": "^4.24.11",
        "react": "^18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "^18.2.0",
        "react-to-print": "^3.0.5",
        "recharts": "^2.15.1",
        "stripe": "^14.0.0"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.10",
        "@types/react": "19.0.10",
        "autoprefixer": "^10.4.14",
        "cross-env": "^7.0.3",
        "eslint": "^8.46.0",
        "eslint-config-next": "^14.0.0",
        "postcss": "^8.4.27",
        "tailwindcss": "^3.3.3"
    }
}
